{
  "name": "moe-torrent-client",
  "version": "1.1.1",
  "description": "Simple react torrent app",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "server": "cross-env NODE_ENV=development node src/server",
    "server:prod": "cross-env NODE_ENV=production node src/server",
    "dev": "npm run build && npm run server",
    "prod": "npm run build:prod && npm run server:prod",
    "start": "npm run server:prod"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "torrent",
    "web",
    "selfhosted",
    "self-hosted"
  ],
  "author": "nathanial292",
